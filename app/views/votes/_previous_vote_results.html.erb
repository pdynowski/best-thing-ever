    <p><%= @prev_loser_score %></p>
  </div>
  <div class="pure-u-1-3" id="loser-picture">
    <img class="last-photo" src="<%=@prev_loser.get_image_url%>">
  </div>
  <div class="pure-u-1-3">
    <p><%= @prev_loser.name %></p>
  </div>
</div>
